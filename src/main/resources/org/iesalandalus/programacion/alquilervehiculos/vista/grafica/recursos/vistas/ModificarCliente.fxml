<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="boton_cancelar" prefHeight="145.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iesalandalus.programacion.alquilervehiculos.vista.grafica.controladores.ModificarCliente">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="10.0">
         <children>
            <Label text="DNI" />
            <TextField fx:id="tfDni" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="29.0" prefWidth="600.0" spacing="10.0">
         <children>
            <Label text="Nombre" />
            <TextField fx:id="tfNombre" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" spacing="10.0">
         <children>
            <Label text="Telefono" />
            <TextField fx:id="tfTelefono" />
         </children>
      </HBox>
      <Button fx:id="boton_aceptar" mnemonicParsing="false" onAction="#aceptar" styleClass="boton_aceptar" stylesheets="@../estilos/javafx.css" text="Aceptar" />
      <Button mnemonicParsing="false" onAction="#cancelar" styleClass="boton_cancelar" stylesheets="@../estilos/javafx.css" text="Cancelar" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
